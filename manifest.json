{
  "manifest_version": 2,

  "name": "Plaid Chrome Extension",
  "description": "Substitutes things for other things",
  "version": "0.1",

  "browser_action": {
    "default_icon": "batman.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.1.1.min.js", "substitute.js"]
  }
]
}